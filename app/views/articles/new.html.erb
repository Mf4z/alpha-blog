<h1>Create New Article</h1>
<%= form_with scope: :article, url: articles_path, local: true  do |form| %>
  Form contents
  <p>
    <%= form.label :title%><br/>
    <%= form.text_field :title%>
  </p>
  <p>
    <%= form.label :description%><br/>
    <%= form.text_area :description%>
  </p>
  <p>
    <%= form.submit%><br/>
  </p>
<% end %>
